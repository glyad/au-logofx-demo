{
  "name": "au-mdc-demo",
  "type": "project:application",
  "paths": {
    "root": "src",
    "resources": "resources",
    "elements": "resources/elements",
    "attributes": "resources/attributes",
    "valueConverters": "resources/value-converters",
    "bindingBehaviors": "resources/binding-behaviors",
    "data": "data",
    "dto": "data/dto",
    "providers": "data/providers",
    "providersContracts": "data/providers/contracts",
    "providersImplementation": "data/providers/implementation",
    "fakeProviders": "data/providers/implementation/fake",
    "realProviders": "data/providers/implementation/real",
    "modelContracts": "model/contracts",
    "modelImplementation": "model/implementation"
  },
  "transpiler": {
    "id": "typescript",
    "fileExtension": ".ts"
  },
  "build": {
    "options": {
      "server": "dev",
      "extractCss": "prod",
      "coverage": false
    }
  },
  "platform": {
    "hmr": false,
    "open": false,
    "port": 8080,
    "output": "dist"
  },
  "packageManager": "npm"
}
